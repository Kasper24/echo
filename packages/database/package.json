  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.20.0",
    "typescript": "5.7.3"
  }
